  },
  "description": "",
  "devDependencies": {
    "@stemn/jest-config": "^0.0.10",
    "@stemn/tsconfig": "^0.0.6",
    "@stemn/tslint-config": "0.0.2",
    "@types/bluebird": "^3.5.23",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.120",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.11",
    "@types/supertest": "^2.0.7",
